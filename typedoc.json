{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./app",
    "./components",
    "./constants",
    "./helpers",
    "./hooks",
    "./services",
    "./store",
    "./styles",
    "./types",
    "./storage.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "name": "AudioScape Documentation",
  "readme": "./README.md",
  "exclude": ["**/node_modules/**", "**/*.test.ts"],
  "excludeExternals": true,
  "excludePrivate": true,
  "hideGenerator": true,
  "plugin": ["typedoc-github-theme"]
}
